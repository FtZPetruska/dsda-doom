{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "dsda-doom",
  "version": "0.29.2",
  "homepage": "https://github.com/kraflab/dsda-doom",
  "license": "GPL-2.0-only",
  "dependencies": [
    {
      "name": "libzip",
      "default-features": false
    },
    "sdl2",
    {
      "name": "sdl2-mixer",
      "default-features": false,
      "features": [
        "fluidsynth",
        "libmodplug",
        "mpg123"
      ]
    },
    "zlib"
  ],
  "features": {
    "fluidsynth": {
      "description": "Build with FluidSynth support",
      "dependencies": [
        {
          "name": "fluidsynth",
          "default-features": false
        }
      ]
    },
    "libmad": {
      "description": "Build with libmad support",
      "dependencies": [
        "libmad"
      ]
    },
    "libopenmpt": {
      "description": "Build with libopenmpt support",
      "dependencies": [
        "libopenmpt"
      ]
    },
    "libvorbis": {
      "description": "Build with vorbisfile support",
      "dependencies": [
        "libvorbis"
      ]
    },
    "portmidi": {
      "description": "Build with PortMidi support",
      "dependencies": [
        "portmidi"
      ]
    },
    "sdl2-image": {
      "description": "Build with SDL2_image support",
      "dependencies": [
        {
          "name": "sdl2-image",
          "default-features": false,
          "features": [
            "libjpeg-turbo"
          ]
        }
      ]
    }
  }
}
